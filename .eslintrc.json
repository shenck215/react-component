{
    "extends": "airbnb",
    "plugins": [
        "react"
    ],
    "globals": {
        "$": true,
        "document": true
    },
    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1}],
        "linebreak-style": 0,
        "no-restricted-syntax": ["error", "WithStatement"],
        "prop-types": ["off"]
    }
}